# Enable rewrite
RewriteEngine on

# Convert subfolder to root get value and redirect to .index
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php?root=$1 [QSA,L]
